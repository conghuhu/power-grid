/* pages/excelTableDeatil/index.wxss */
.file-container {
  padding: 15rpx;
}

.file-container .prop-content{
  display: flex;
  flex-direction: column;
}

.navTop {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}

.navTop .title {
  font-size: large;
  font-weight: 500;
}

.navTop .iconList {
  display: flex;
  justify-content: center;
  align-items: center;
}

.deadLine {
  font-size: small;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  color: rgb(128, 126, 125);
}

.sousuo {
  font-size: 45rpx;
  padding: 10rpx;
}

.set {
  font-size: 50rpx;
  padding: 10rpx;
}

.export {
  font-size: 47rpx;
  padding: 10rpx;
  color: var(--primary);
}

.loading-container {
  width: 100%;
  height: 90vh;
  display: flex;
  justify-content: center;
  align-items: center;
}
.pagination{
  margin-top: 10rpx;
  display: flex;
  width: 100%;
  justify-content: flex-end;
  align-items: center;
}